                <TabItem Header="Termo de Responsabilidade" HorizontalAlignment="Left" Height="20" VerticalAlignment="Top" Width="158" Margin="19,0,-19,0">
                    <Grid Background="White">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="533*"/>
